        }
    },
    "autoload-dev": {
        "psr-4": {
            "AmirNajmi\\ContactUs\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmirNajmi\\ContactUs\\ContactUsServiceProvider"
            ],
            "aliases": {
                "ContactUs": "AmirNajmi\\ContactUs\\Facades\\ContactUs"
            }
        }
    }
}
