{
    "name": "amirnajmi/contactus",
    "description": ":package_description",
    "license": "MIT",
    "authors": [
        {
            "name": "Amir Najmi",
            "email": "amirmo.naj@gmail.com",
        }
    ],  
    "minimum-stability": "dev",
    "homepage": "https://github.com/amirnajmi/contactus",
    "keywords": ["Laravel", "ContactUs"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
